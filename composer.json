{
    "name": "starterteam/starter-nessa",
    "type": "typo3-cms-extension",
    "description": "Theme extension for EXT:starter with bootstrap",
    "license": "proprietary",
    "authors": [
        {
            "name": "Marcel Schmid",
            "email": "m.schmid@visual-fx.ch"
        },
        {
            "name": "Christian Wolfram",
            "email": "c.wolfram@chriwo.de"
        }
    ],
    "support": {
        "issues": "https://github.com/chriwo/starter-nessa/issues"
    },
    "require": {
		"php": "^8.3",
		"typo3/cms-core": "^13.4"
    },
	"require-dev": {
		"helhum/dotenv-connector": "^3.1",
		"phpstan/extension-installer": "^1.4",
		"phpstan/phpstan": "^2.1",
		"saschaegerer/phpstan-typo3": "^2.1",
		"typo3/coding-standards": "^0.8.0",
		"typo3/testing-framework": "^8.2",
		"typo3/cms-adminpanel": "^13.4",
		"typo3/cms-backend": "^13.4",
		"typo3/cms-belog": "^13.4",
		"typo3/cms-beuser": "^13.4",
		"typo3/cms-dashboard": "^13.4",
		"typo3/cms-extbase": "^13.4",
		"typo3/cms-extensionmanager": "^13.4",
		"typo3/cms-felogin": "^13.4",
		"typo3/cms-filelist": "^13.4",
		"typo3/cms-fluid": "^13.4",
		"typo3/cms-fluid-styled-content": "^13.4",
		"typo3/cms-frontend": "^13.4",
		"typo3/cms-impexp": "^13.4",
		"typo3/cms-info": "^13.4",
		"typo3/cms-lowlevel": "^13.4",
		"typo3/cms-recordlist": "^13.4",
		"typo3/cms-recycler": "^13.4",
		"typo3/cms-rte-ckeditor": "^13.4",
		"typo3/cms-scheduler": "^13.4",
		"typo3/cms-seo": "^13.4",
		"typo3/cms-setup": "^13.4",
		"typo3/cms-t3editor": "^13.4",
		"typo3/cms-tstemplate": "^13.4",
        "helhum/typo3-console": "^8.3",
        "helmich/typo3-typoscript-lint": "^3.3",
        "michielroos/typo3scan": "^1.7",
        "phpstan/phpstan-phpunit": "^2.0",
        "ssch/typo3-rector": "^3.9",
        "a9f/typo3-fractor": "^0.5.8"
	},
    "autoload": {
        "psr-4": {
            "StarterTeam\\StarterNessa\\": "Classes"
        }
    },
	"autoload-dev": {
		"psr-4": {
			"StarterTeam\\StarterNessa\\Tests\\": "Tests"
		}
	},
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin",
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true,
            "helhum/dotenv-connector": true,
            "phpstan/extension-installer": true,
            "a9f/fractor-extension-installer": true
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "starter_nessa",
            "web-dir": ".Build/public"
        }
    }
}
